
@*Priyanshu*@
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Dashboard</title>
    <link href="~/Content/assets/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/Content/assets/css/style.css" rel="stylesheet">
    <link href="~/Content/assets/css/jquery-confirm.css" rel="stylesheet" />
    <link href="~/Content/assets/css/colors/blue.css" id="theme" rel="stylesheet">
    <link href="~/Content/assets/css/custom-dev.css" rel="stylesheet">
    <link href="~/Content/assets/css/multi-select2-styles.css" rel="stylesheet" />
    <link href="~/Content/assets/css/select2.min.css" rel="stylesheet" />
    @*<link href="~/Content/assets/css/custom.css" rel="stylesheet">*@
    <link rel="icon" type="image/png" sizes="16x16" href="../Content/assets/images/favicon.png" />

    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <style>
        h3 {
            line-height: 51px;
            font-size: 21px;
            vertical-align: middle;
        }
    </style>
</head>
<body class="fix-header fix-sidebar card-no-border">
    @*<div class="preloader">
        <svg class="circular" viewBox="25 25 50 50">
            <circle class="path" cx="50" cy="50" r="20" fill="none" stroke-width="2" stroke-miterlimit="10" />
        </svg>
    </div>*@
    <div id="main-wrapper">
        <header class="topbar">
            <nav class="navbar top-navbar navbar-expand-md navbar-light">

                <div class="navbar-header">
                    <a class="navbar-brand" href="http://micromap.quaeretech.com/">
                        <img src="~/content/assets/images/logo-micromap.png" alt="Logo" class="dark-logo">

                    </a>
                </div>

                <!-- ============================================================== -->
                <!-- End Logo -->
                <!-- ============================================================== -->
                <div class="navbar-collapse">
                    <!-- ============================================================== -->
                    <!-- toggle and nav items -->
                    <!-- ============================================================== -->
                    <ul class="navbar-nav mr-auto mt-md-0">
                        <!-- This is  -->
                        <li class="nav-item"> <a class="nav-link nav-toggler hidden-md-up text-muted waves-effect waves-dark" href="javascript:void(0)"><i class="mdi mdi-menu"></i></a> </li>
                        <li class="nav-item m-l-10"> <a class="nav-link sidebartoggler hidden-sm-down text-muted waves-effect waves-dark" href="javascript:void(0)"><i class="ti-menu"></i></a> </li>
                        @*<h3 style="color:white">
                            Micromap e-Management System
                        </h3>*@

                    </ul>
                    <!-- ============================================================== -->
                    <!-- User profile and search -->
                    <!-- ============================================================== -->
                    <ul class="navbar-nav my-lg-0">
                        <!-- ============================================================== -->
                        <!-- Search -->
                        <!-- ============================================================== -->
                        <li class="nav-item hidden-sm-down search-box">
                            <a class="nav-link hidden-sm-down text-muted waves-effect waves-dark" href="javascript:void(0)"><i class="ti-search"></i></a>
                            <form class="app-search">
                                <input type="text" class="form-control" placeholder="Search & enter"> <a class="srh-btn"><i class="ti-close"></i></a>
                            </form>
                        </li>

                        <!-- ============================================================== -->
                        <!-- Profile -->
                        <!-- ============================================================== -->
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle text-muted waves-effect waves-dark" href="#" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><img src="~/Content/assets/images/user1.jpeg" alt="user" class="profile-pic" /></a>
                            <div class="dropdown-menu dropdown-menu-right scale-up">
                                <ul class="dropdown-user">
                                    <li>
                                        <div class="dw-user-box">
                                            <div class="u-img"><img src="~/Content/assets/images/user1.jpeg" alt="user"></div>
                                            <div class="u-text">
                                                @*<h4>@sm.Name</h4>
                                                <p class="text-muted">@sm.Email</p><a href="#" class="btn btn-rounded btn-danger btn-sm">View Profile</a>*@
                                            </div>
                                        </div>
                                    </li>
                                    <li><a href="#"><i class="ti-user"></i> My Profile</a></li>

                                    <li><a href="#"><i class="ti-settings"></i> Account Setting</a></li>
                                    <li><a href="/Account/Login"><i class="fa fa-power-off"></i> Logout</a></li>
                                </ul>
                            </div>
                        </li>
                    </ul>
                </div>
            </nav>
        </header>

        <aside class="left-sidebar" style="overflow: scroll!important;">
            <!-- Sidebar scroll-->
            <div class="scroll-sidebar">
                <!-- User profile -->
                <div class="user-profile">
                    <!-- User profile image -->
                    <div class="profile-img">
                        <img src="~/Content/assets/images/user1.jpeg" alt="user" />
                        <!-- this is blinking heartbit-->
                        <div class="notify setpos"> <span class="heartbit"></span> <span class="point"></span> </div>
                    </div>
                    <!-- User profile text-->
                    <div class="profile-text">
                        @*<h5>@sm.Name</h5>*@
                        <a href="#" class="dropdown-toggle u-dropdown" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="true"><i class="mdi mdi-settings"></i></a>

                        <a href="/Account/Login" class="" data-toggle="tooltip" title="Logout"><i class="mdi mdi-power"></i></a>

                    </div>
                </div>
                <!-- End User profile text-->
                <!-- Sidebar navigation-->
                <nav class="sidebar-nav">
                    <ul id="sidebarnav">
                        <li>
                            <a class="waves-effect waves-dark" href="/Admin/Dashboard"><i class="mdi mdi-gauge"></i><span class="hide-menu">Dashboard</span></a>
                        </li>


                    </ul>
                </nav>
                <!-- End Sidebar navigation -->
            </div>
            <!-- End Sidebar scroll-->
        </aside>
        <div class="page-wrapper">
            @RenderBody()
            <footer class="footer text-center">
                Copyright © @DateTime.Now.Year.ToString(), <strong>MicroMap </strong> | All Rights Reserved

            </footer>
        </div>
    </div>

</body>
</html>
<script src="~/Content/assets/js/jquery-confirm.min.js"></script>
<script src="~/Scripts/jquery.validate.min.js"></script>
<script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
<script src="~/Content/assets/plugins/datatables/jquery.dataTables.min.js"></script>
<!-- Bootstrap tether Core JavaScript -->
<script src="../Content/assets/plugins/bootstrap/js/popper.min.js"></script>
<script src="../Content/assets/plugins/bootstrap/js/bootstrap.min.js"></script>
<script src="~/Content/assets/js/jquery.multi-select.js"></script>
<script src="~/Content/assets/js/jquery.multi-select.min.js"></script>
<script src="~/Content/assets/js/select2.min.js"></script>
<!-- slimscrollbar scrollbar JavaScript -->
<!--Wave Effects -->
<script src="../Content/assets/js/waves.js"></script>
<!--Menu sidebar -->
<script src="../Content/assets/js/sidebarmenu.js"></script>
<!--stickey kit -->
<script src="../Content/assets/plugins/sticky-kit-master/dist/sticky-kit.min.js"></script>
<script src="../Content/assets/plugins/sparkline/jquery.sparkline.min.js"></script>
<!--Custom JavaScript -->
<script src="../Content/assets/js/custom.min.js"></script>
<!-- ============================================================== -->
<!-- Style switcher -->
<!-- ============================================================== -->
<script src="../Content/assets/plugins/styleswitcher/jQuery.style.switcher.js"></script>
<script src="~/Scripts/PDF/dist/jspdf.debug.js"></script>
<script src="~/Scripts/PDF/dist/jspdf.js"></script>
<script src="~/Scripts/PDF/dist/jspdf.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/2.1.2/sweetalert.min.js" integrity="sha512-AA1Bzp5Q0K1KanKKmvN/4d3IRKVlv9PYgwFPvm32nPO6QS8yH1HO7LbgB1pgiOxPtfeg5zEn2ba64MUcqJx6CA==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
<script>
    $(document).ready(function () {
        $('.datatable').DataTable();
    });
</script>
